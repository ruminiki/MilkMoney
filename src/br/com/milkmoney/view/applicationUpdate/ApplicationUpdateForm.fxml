                              <Insets />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="btnFinalizar" mnemonicParsing="false" onAction="#handleCancel" text="Agora Não">
                           <HBox.margin>
                              <Insets right="2.0" />
                           </HBox.margin></Button>
                     </children>
                     <padding>
                        <Insets bottom="5.0" right="3.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
