<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="425.0" prefWidth="707.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.milksys.controller.EntregaLeiteController">
   <children>
      <VBox prefHeight="425.0" prefWidth="665.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="lblPeriodo" nodeOrientation="LEFT_TO_RIGHT" text="Label" textAlignment="RIGHT">
               <VBox.margin>
                  <Insets left="5.0" top="5.0" />
               </VBox.margin>
            </Label>
            <HBox alignment="CENTER_LEFT" spacing="5.0">
               <children>
                  <Label text="Mês Referência" />
                  <ComboBox fx:id="inputMesReferencia" prefWidth="150.0" />
                  <Label text="Ano Referência" />
                  <TextField fx:id="inputAno" alignment="CENTER" maxWidth="65.0" minHeight="25.0" promptText="Ano" />
               </children>
               <padding>
                  <Insets left="5.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </HBox>
            <TableView fx:id="table" editable="true" VBox.vgrow="SOMETIMES">
              <columns>
                <TableColumn fx:id="dataColumn" maxWidth="-1.0" minWidth="100.0" prefWidth="-1.0" resizable="false" sortable="false" style="-fx-alignment: CENTER;" text="Data" />
                <TableColumn fx:id="numeroVacasOrdenhadasColumn" maxWidth="-1.0" minWidth="100.0" prefWidth="-1.0" style="-fx-alignment: CENTER;" text="Nº Vacas" />
                  <TableColumn fx:id="volumeColumn" maxWidth="-1.0" minWidth="100.0" prefWidth="-1.0" style="-fx-alignment: CENTER;" text="Volume" />
                  <TableColumn fx:id="observacaoColumn" maxWidth="-1.0" minWidth="300.0" prefWidth="-1.0" text="Observaçao" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <VBox.margin>
                  <Insets bottom="10.0" left="5.0" right="5.0" />
               </VBox.margin>
            </TableView>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </children>
</AnchorPane>
